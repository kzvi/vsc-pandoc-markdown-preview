{
	"name": "pandoc-markdown-preview",
	"displayName": "Pandoc Markdown Preview",
	"description": "",
	"version": "0.0.1",
	"repository": "https://github.com/kzvi/vsc-pandoc-markdown-preview",
	"publisher": "kzvi",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openPandocMarkdownPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openPandocMarkdownPreview",
				"title": "Open Pandoc Markdown Preview",
				"when": "editorLangId == markdown"
			}
		],
		"keybindings": [
			{
				"command": "extension.openPandocMarkdownPreview",
				"key": "shift+ctrl+r",
				"mac": "shift+cmd+r",
				"when": "editorTextFocus && editorLangId == markdown"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"katex": "^0.11.1"
	}
}
